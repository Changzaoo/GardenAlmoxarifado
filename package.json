    "build:win": "set CI=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:version": "node scripts/generate-version.js && cross-env CI=false react-scripts build",
    "vercel-build": "npm run build:version",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "engines": {
    "node": "22.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
