{
  "name": "almoxarifado-jardim",
  "version": "1.0.0",
  "description": "Sistema de controle de almoxarifado para ferramentas e equipamentos de jardim",
  "private": true,
  "dependencies": {
    "crypto-js": "^4.2.0",
    "firebase": "^12.1.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.3.2"
  },
  "homepage": "https://changzaoo.github.io/gardenalmoxarifado",
  "scripts": {
    "preinstall": "git config --global --add safe.directory /vercel/path0 && git submodule sync --recursive",
  "install": "npm install --legacy-peer-deps",
    "start": "react-scripts start",
    "build": "npm run build && react-scripts build && cp _redirects build/",
    "postbuild": "workbox generateSW workbox-config.js",
    "test": "react-scripts test",
    "deploy": "gh-pages -d build -r https://github.com/changzaoo/gardenalmoxarifado",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12"
  }
}
