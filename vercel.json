{
  "version": 2,
  "routes": [
    {
      "src": "/index.js",
      "headers": { "Content-Type": "application/javascript" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": { "Content-Type": "text/html" }
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}