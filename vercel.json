{
  "version": 2,
  "installCommand": "npm ci --force",
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}