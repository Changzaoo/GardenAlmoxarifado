{
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|json|svg|png|jpg|jpeg|gif|ico|html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "REACT_APP_ENV": "production",
    "REACT_APP_FIREBASE_API_KEY": "AIzaSyAnLmtlhOUUAbtRcOg64dXdCLbltv_iE4E",
    "REACT_APP_FIREBASE_AUTH_DOMAIN": "garden-c0b50.firebaseapp.com",
    "REACT_APP_FIREBASE_PROJECT_ID": "garden-c0b50",
    "REACT_APP_FIREBASE_STORAGE_BUCKET": "garden-c0b50.firebasestorage.app",
    "REACT_APP_FIREBASE_MESSAGING_SENDER_ID": "467344354997",
    "REACT_APP_FIREBASE_APP_ID": "1:467344354997:web:3c3397e0176060bb0c98fc",
    "REACT_APP_FIREBASE_MEASUREMENT_ID": "G-7LML93QDTF"
  }
}
